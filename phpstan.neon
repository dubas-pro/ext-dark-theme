parameters:
  phpVersion: 80100 # PHP 8.1
  level: max
  paths:
    - src

  excludePaths:
    analyse:
      - site
      - src/*/vendor/*

  bootstrapFiles:
    - site/bootstrap.php

  inferPrivatePropertyTypeFromConstructor: true
  treatPhpDocTypesAsCertain: false
  reportUnmatchedIgnoredErrors: false
